%table(cellpadding=0 cellspacing=0)
  %tr
    %th.name Name
    %th.version Version
    %th.cid CID
    
  - @stemcells.each do |stemcell|
    %tr{ :class => cycle("odd", "even") }
      %td= h stemcell.name
      %td= h stemcell.version
      %td= h stemcell.cid

  %tr
    %td.total(colspan=3)= "Total: %s" % [ @stemcells.size ]
