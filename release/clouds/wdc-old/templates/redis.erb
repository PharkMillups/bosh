daemonize no
pidfile <%= redis_deploy_to %>/redis.pid
port 25255
timeout 300
loglevel notice
logfile stdout
databases 16
dir <%= redis_deploy_to %>
requirepass R3d!S
maxclients 0
maxmemory-policy noeviction
appendonly yes
appendfilename appendonly.aof
appendfsync everysec
no-appendfsync-on-rewrite no